  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "apollo-server-express": "^2.25.2",
    "graphql": "^15.5.3",
    "if-env": "^1.0.4",
    "react-bootstrap": "^1.6.3",
    "react-icons": "^4.2.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0"
  }
}
